		lv2:symbol "event_in" ;
		lv2:name "Event In" ;
		lv2:designation lv2:control ;
	] , [
		# event out
	  a lv2:OutputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		lv2:index 1 ;
		lv2:symbol "event_out" ;
		lv2:name "Event Out" ;
		lv2:designation lv2:control ;
	] , [
		# control input ports
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 2 ;
		lv2:symbol "octave" ;
		lv2:name "Octave" ;
		lv2:default 3 ;
		lv2:minimum 0 ;
		lv2:maximum 6 ;
		lv2:portProperty lv2:integer ;
		units:unit units:oct ;
	] , [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 3 ;
		lv2:symbol "channel" ;
