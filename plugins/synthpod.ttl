osc:Event
	a rdfs:Class ;
	rdfs:subClassOf atom:Object ;
	rdfs:label "OSC Event (Bundle or Message)" .

# to please sord_validate
zwork:interface
	a lv2:ExtensionData .
zwork:schedule
	a lv2:Feature .

zwrite:schedule
	a lv2:Feature .

# Maintainer
omk:me
	a foaf:Person ;
	foaf:name "Hanspeter Portner" ;
	foaf:mbox <mailto:dev@open-music-kontrollers.ch> ;
	foaf:homepage <http://open-music-kontrollers.ch> .

# Project
proj:synthpod
	a doap:Project ;
	doap:maintainer omk:me ;
	doap:name "Synthpod Bundle" .

# Audio groups
synthpod:group_in
	a pg:InputGroup ,
		pg:StereoGroup ;
	lv2:name "Input" ;
	lv2:symbol "in" .

synthpod:group_out
	a pg:OutputGroup ,
		pg:StereoGroup ;
	lv2:name "Output" ;
	lv2:symbol "out" .

# Common UIs
synthpod:common_eo
	a ui:EoUI ;
	ui:portNotification [
		ui:plugin synthpod:stereo ;
		ui:plugin synthpod:monoatom ;
		lv2:symbol "notify" ;
		ui:protocol atom:eventTransfer ;
	] ;
	lv2:optionalFeature inst:, zwrite:schedule ;
	lv2:requiredFeature ui:portMap, urid:map .
synthpod:common_ui
	a ui:UI ;
	ui:portNotification [
		ui:plugin synthpod:stereo ;
		ui:plugin synthpod:monoatom ;
		lv2:symbol "notify" ;
		ui:protocol atom:eventTransfer ;
	] ;
	lv2:optionalFeature inst:, zwrite:schedule ;
	lv2:requiredFeature ui:idleInterface, ui:portMap, urid:map ;
  lv2:extensionData ui:idleInterface, ui:showInterface .
synthpod:common_x11
	a ui:X11UI ;
	ui:portNotification [
		ui:plugin synthpod:stereo ;
		ui:plugin synthpod:monoatom ;
		lv2:symbol "notify" ;
		ui:protocol atom:eventTransfer ;
	] ;
	lv2:optionalFeature ui:resize, inst:, zwrite:schedule ;
	lv2:requiredFeature ui:idleInterface, ui:portMap, urid:map ;
  lv2:extensionData ui:idleInterface .
synthpod:common_kx
	a kx:Widget ;
	ui:portNotification [
		ui:plugin synthpod:stereo ;
		ui:plugin synthpod:monoatom ;
		lv2:symbol "notify" ;
		ui:protocol atom:eventTransfer ;
	] ;
	lv2:optionalFeature inst:, zwrite:schedule ;
	lv2:requiredFeature kx:Host, ui:portMap, urid:map .

# Stereo Plugin
synthpod:stereo
	a lv2:Plugin ,
		lv2:InstrumentPlugin ;
	doap:name "Synthpod Stereo" ;
	doap:license lic:Artistic-2.0 ;
	lv2:project proj:synthpod ;
	lv2:extensionData work:interface , state:interface, zwork:interface, opts:interface ;
	lv2:requiredFeature work:schedule , bufsz:boundedBlockLength, urid:map, urid:unmap ;
	lv2:optionalFeature lv2:isLive , lv2:hardRTCapable , log:log, zwork:schedule ;

	lv2:port [
		# event in
	  a lv2:InputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		atom:supports osc:Event ;
		atom:supports atom:Object ;
		atom:supports time:Position ;
		atom:supports patch:Message ;
		lv2:index 0 ;
		lv2:symbol "event_in" ;
		lv2:name "Event In" ;
		lv2:designation lv2:control ;
		pg:group synthpod:group_in ;
	] , [
		# event out
	  a lv2:OutputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports midi:MidiEvent ;
		atom:supports osc:Event ;
		atom:supports atom:Object ;
		atom:supports time:Position ;
		atom:supports patch:Message ;
		lv2:index 1 ;
		lv2:symbol "event_out" ;
		lv2:name "Event Out" ;
		lv2:designation lv2:control ;
		pg:group synthpod:group_out ;
	] , [
		# audio in 1
	  a lv2:InputPort ,
			lv2:AudioPort ;
		lv2:index 2 ;
		lv2:symbol "audio_in_1" ;
		lv2:name "Audio In 1" ;
		lv2:designation pg:left ;
		pg:group synthpod:group_in ;
	] , [
		# audio in 2
	  a lv2:InputPort ,
			lv2:AudioPort ;
		lv2:index 3 ;
		lv2:symbol "audio_in_2" ;
		lv2:name "Audio In 2" ;
		lv2:designation pg:right ;
		pg:group synthpod:group_in ;
	] , [
		# audio out 1
	  a lv2:OutputPort ,
			lv2:AudioPort ;
		lv2:index 4 ;
		lv2:symbol "audio_out_1" ;
		lv2:name "Audio Out 1" ;
		lv2:designation pg:left ;
		pg:group synthpod:group_out ;
	] , [
		# audio out 2
	  a lv2:OutputPort ,
			lv2:AudioPort ;
		lv2:index 5 ;
		lv2:symbol "audio_out_2" ;
		lv2:name "Audio Out 2" ;
		lv2:designation pg:right ;
		pg:group synthpod:group_out ;
	] , [
		# control in
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 6 ;
		lv2:symbol "control_in_1" ;
		lv2:name "Input 1" ;
		pg:group synthpod:group_in ;
	] , [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 7 ;
		lv2:symbol "control_in_2" ;
		lv2:name "Input 2" ;
		pg:group synthpod:group_in ;
	] , [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 8 ;
		lv2:symbol "control_in_3" ;
		lv2:name "Input 3" ;
		pg:group synthpod:group_in ;
	] , [
	  a lv2:InputPort ,
			lv2:ControlPort ;
		lv2:index 9 ;
		lv2:symbol "control_in_4" ;
		lv2:name "Input 4" ;
		pg:group synthpod:group_in ;
	] , [
		# control out
	  a lv2:OutputPort ,
			lv2:ControlPort ;
		lv2:index 10 ;
		lv2:symbol "control_out_1" ;
		lv2:name "Output 1" ;
		pg:group synthpod:group_out ;
	] , [
	  a lv2:OutputPort ,
			lv2:ControlPort ;
		lv2:index 11 ;
		lv2:symbol "control_out_2" ;
		lv2:name "Output 2" ;
		pg:group synthpod:group_out ;
	] , [
	  a lv2:OutputPort ,
			lv2:ControlPort ;
		lv2:index 12 ;
		lv2:symbol "control_out_3" ;
		lv2:name "Output 3" ;
		pg:group synthpod:group_out ;
	] , [
	  a lv2:OutputPort ,
			lv2:ControlPort ;
		lv2:index 13 ;
		lv2:symbol "control_out_4" ;
		lv2:name "Output 4" ;
		pg:group synthpod:group_out ;
	] , [
		# control event port
	  a lv2:InputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports atom:Object ;
		atom:supports patch:Message ;
		lv2:index 14 ;
		lv2:symbol "control" ;
		lv2:name "Control" ;
		rsz:minimumSize 65536;
	] , [
		# notify event port
	  a lv2:OutputPort ,
			atom:AtomPort ;
		atom:bufferType atom:Sequence ;
		atom:supports atom:Object ;
		atom:supports patch:Message ;
		lv2:index 15 ;
		lv2:symbol "notify" ;
		lv2:name "Notify" ;
		rsz:minimumSize 65536;
	] .

